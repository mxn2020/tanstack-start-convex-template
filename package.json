{
  "name": "tanstack-start-example-convex-trellaux",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "npx convex dev --once && npm run db:generate && concurrently -r npm:dev:web npm:dev:db",
    "dev:web": "vite dev",
    "dev:db": "convex dev --run board:seed",
    "build": "npm run db:generate && vite build && tsc --noEmit",
